<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sign.login">

    <application
        android:name=".application.LoginApp"
        android:allowBackup="false"
        android:icon="@mipmap/login_ic_launcher"
        android:label="@string/login_app_name"
        android:roundIcon="@mipmap/login_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/LoginTheme.HelloComponent">
        <activity
            android:name=".LoginActivity"
            android:label="登陆">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>